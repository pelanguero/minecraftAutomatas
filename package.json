{
    "name": "minecraftlegion",
    "version": "1.2.0",
    "description": "Bot multi proposito para minecraft",
    "main": "index.js",
    "directories": {
        "example": "examples"
    },
    "dependencies": {
        "express": "4.18.1",
        "lowdb": "1.0.0",
        "minecraft-data": "3.9.1",
        "minecrafthawkeye": "1.2.7",
        "mineflayer": "4.3.0",
        "mineflayer-pathfinder": "2.2.0",
        "mineflayer-statemachine": "1.6.5",
        "mineflayer-web-inventory": "1.8.3",
        "module-alias": "2.2.2",
        "prismarine-viewer": "1.23.0",
        "socket.io-client": "4.5.1",
        "vec3": "0.1.7"
    },
    "devDependencies": {
        "nodemon": "2.0.19",
        "standard": "17.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    },
    "scripts": {
        "start": "nodemon src/index.ts --ignore botConfig/ --ignore custom_start/",
        "one": "nodemon --inspect=0.0.0.0:9229 src/start_bot.ts $1 $2 --ignore src/botConfig/ --ignore src/custom_start/",
        "test": "echo \"Error: no test specified\" && exit 1",
        "standard": "standard --fix",
        "dockerrun": "docker run --rm -p:3000:3000 minecraftlegion",
        "docker": "docker run --rm -it --name minecraftLegion  -v ~/.gitconfig:/etc/gitconfig --network minecraftLegionNetwork -v $PWD:/home/app -w /home/app -u node -p 4500-4550:4500-4550 -p 9229:9229 node:16.13-bullseye /bin/bash",
        "update": "npx npm-check-updates",
        "ts": "ts-node ./src/start_bot $1 $2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sefirosweb/minecraftLegion.git"
    },
    "author": "Sefirosweb",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/sefirosweb/minecraftLegion/issues"
    },
    "homepage": "https://github.com/sefirosweb/minecraftLegion#readme",
    "_moduleAliases": {
        "@": "./src/",
        "@modules": "./src/modules",
        "@BehaviorModules": "./src/BehaviorModules",
        "@NestedStateModules": "./src/NestedStateModules",
        "@config": "./src/config"
    }
}
