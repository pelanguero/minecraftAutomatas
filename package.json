{
    "name": "minecraftlegion",
    "version": "1.2.0",
    "description": "Bot multi proposito para minecraft",
    "main": "index.js",
    "directories": {
        "example": "examples"
    },
    "dependencies": {
        "express": "4.17.3",
        "lowdb": "1.0.0",
        "minecraft-data": "3.0.0",
        "minecrafthawkeye": "1.2.7",
        "mineflayer": "4.2.0",
        "mineflayer-pathfinder": "2.0.0",
        "mineflayer-statemachine": "1.6.5",
        "mineflayer-web-inventory": "1.7.1",
        "module-alias": "2.2.2",
        "prismarine-viewer": "1.22.0",
        "socket.io-client": "4.4.1",
        "vec3": "0.1.7"
    },
    "devDependencies": {
        "nodemon": "2.0.15",
        "standard": "16.0.4"
    },
    "scripts": {
        "start": "nodemon index.js --ignore botConfig/ --ignore custom_start/",
        "one": "nodemon --inspect=0.0.0.0:9229 start_bot.js Miner password --ignore botConfig/ --ignore custom_start/",
        "test": "echo \"Error: no test specified\" && exit 1",
        "standard": "standard --fix",
        "dockerrun": "docker run --rm -p:3000:3000 minecraftlegion",
        "docker": "docker run --rm -it --name minecraftLegion  -v ~/.gitconfig:/etc/gitconfig --network minecraftLegionNetwork -v $PWD:/home/app -w /home/app -u node -p 4500-4550:4500-4550 -p 9229:9229 node:16.13-bullseye /bin/bash",
        "update": "npx npm-check-updates",
        "craft": "nodemon --inspect=0.0.0.0:9229 ./trash_files/craft.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sefirosweb/minecraftLegion.git"
    },
    "author": "Sefirosweb",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/sefirosweb/minecraftLegion/issues"
    },
    "homepage": "https://github.com/sefirosweb/minecraftLegion#readme",
    "_moduleAliases": {
        "@": ".",
        "@modules": "./modules",
        "@BehaviorModules": "./BehaviorModules",
        "@NestedStateModules": "./NestedStateModules",
        "@config": "./config"
    }
}