    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "minecraft-data": "^3.47.0",
    "minecraft-wrap": "^1.5.1",
    "mineflayer": "^4.14.0",
    "mineflayer-pathfinder": "^2.4.5",
    "mineflayer-statemachine": "^1.7.0",
    "prismarine-block": "^1.17.1",
