{
    "name": "minecraftlegion",
    "version": "1.2.0",
    "description": "Bot multi proposito para minecraft",
    "main": "index.js",
    "directories": {
        "example": "examples"
    },
    "dependencies": {
        "express": "4.18.1",
        "lowdb": "1.0.0",
        "minecraft-data": "3.10.2",
        "minecrafthawkeye": "1.2.7",
        "mineflayer": "4.4.0",
        "mineflayer-pathfinder": "2.3.1",
        "mineflayer-statemachine": "1.6.5",
        "mineflayer-web-inventory": "1.8.3",
        "module-alias": "2.2.2",
        "prismarine-viewer": "1.23.0",
        "socket.io-client": "4.5.1",
        "vec3": "0.1.7"
    },
    "devDependencies": {
        "@types/chai": "^4.3.3",
        "@types/events": "^3.0.0",
        "@types/lodash": "^4.14.184",
        "@types/mocha": "^9.1.1",
        "chai": "^4.3.7",
        "events": "^3.3.0",
        "lodash": "^4.17.21",
        "minecraft-wrap": "^1.5.1",
        "mocha": "^10.2.0",
        "nodemon": "2.0.19",
        "prismarine-block": "^1.16.3",
        "prismarine-entity": "^2.1.1",
        "standard": "17.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    },
    "scripts": {
        "start": "nodemon src/index.ts --ignore botConfig/ --ignore custom_start/ --ignore test/",
        "one": "nodemon --inspect=0.0.0.0:9229 src/start_bot.ts $1 $2",
        "nodemon": "nodemon --inspect=0.0.0.0:9229 $1",
        "start_server_test": "docker build -t docker_minecraftlegion . && docker run --rm -it --name minecraftLegion_test_server --network minecraftLegionNetwork -v $PWD:/home/app -w /home/app -u node -p 25565:25565 docker_minecraftlegion ts-node test/common/start_test_server.ts",
        "test": "mocha -r ts-node/register -r test/hooks.ts --inspect=0.0.0.0:9229 --exit",
        "test_all": "npm run test 'test/legionTests/*.ts'",
        "standard": "standard --fix",
        "dockerrun": "docker run --rm -p:3000:3000 minecraftlegion",
        "docker": "docker build -t docker_minecraftlegion . && docker run --rm -it --name minecraftLegion  -v ~/.gitconfig:/etc/gitconfig --network minecraftLegionNetwork -v $PWD:/home/app -w /home/app -u node -p 4500-4550:4500-4550 -p 9229:9229 docker_minecraftlegion /bin/bash",
        "dockerbuild": "docker build -t docker_minecraftlegion .",
        "update": "npx npm-check-updates",
        "lint": "standard && standard-markdown",
        "ts": "ts-node ./src/start_bot $1 $2"
    },
    "standard": {
        "ignore": [
            "/src/trash_files/**/*.js"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sefirosweb/minecraftLegion.git"
    },
    "author": "Sefirosweb",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/sefirosweb/minecraftLegion/issues"
    },
    "homepage": "https://github.com/sefirosweb/minecraftLegion#readme",
    "_moduleAliases": {
        "@": "./src/",
        "@modules": "./src/modules",
        "@BehaviorModules": "./src/BehaviorModules",
        "@NestedStateModules": "./src/NestedStateModules",
        "@config": "./src/config"
    }
}
