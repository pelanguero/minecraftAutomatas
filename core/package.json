{
    "name": "minecraftlegion",
    "version": "1.2.0",
    "description": "Bot multi proposito para minecraft",
    "main": "index.js",
    "directories": {
        "example": "examples"
    },
    "scripts": {
        "start": "nodemon src/index.ts --ignore botConfig/ --ignore custom_start/",
        "one": "nodemon --inspect=0.0.0.0:9229 src/start_bot.ts $1 $2",
        "test": "mocha -r tsconfig-paths/register -r ts-node/register -r test/hooks.ts --inspect=0.0.0.0:9229 --exit",
        "test:all": "npm run test 'test/legionTests/*.ts'",
        "standard": "standard --fix",
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "lint:fix": "npm run lint -- --fix",
        "build": "babel src --extensions \".ts,.js\" --delete-dir-on-start --out-dir dist",
        "ts": "ts-node -r tsconfig-paths/register ./src/start_bot $1 $2"
    },
    "dependencies": {
        "async": "^3.2.4",
        "base-types": "file:../base-types",
        "dotenv": "^16.0.3",
        "express": "4.18.2",
        "lowdb": "1.0.0",
        "minecraft-data": "3.30.0",
        "minecrafthawkeye": "1.3.5",
        "mineflayer": "4.8.1",
        "mineflayer-pathfinder": "2.4.2",
        "mineflayer-statemachine": "1.7.0",
        "mineflayer-web-inventory": "1.8.4",
        "prismarine-viewer": "1.24.0",
        "socket.io-client": "4.6.1",
        "uuid": "^9.0.0",
        "vec3": "0.1.8"
    },
    "devDependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/core": "^7.21.0",
        "@babel/node": "^7.20.7",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.21.0",
        "@types/async": "^3.2.18",
        "@types/chai": "^4.3.4",
        "@types/events": "^3.0.0",
        "@types/lodash": "^4.14.191",
        "@types/mocha": "^10.0.1",
        "@types/uuid": "^9.0.1",
        "babel-plugin-module-resolver": "^5.0.0",
        "chai": "^4.3.7",
        "events": "^3.3.0",
        "lodash": "^4.17.21",
        "mocha": "^10.2.0",
        "nodemon": "2.0.21",
        "prismarine-block": "^1.16.3",
        "prismarine-entity": "^2.2.0",
        "prismarine-windows": "^2.6.1",
        "swc": "^1.0.11",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.2",
        "tslint": "^6.1.3",
        "typescript": "^4.9.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sefirosweb/minecraftLegion.git"
    },
    "author": "Sefirosweb",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/sefirosweb/minecraftLegion/issues"
    },
    "homepage": "https://github.com/sefirosweb/minecraftLegion#readme"
}
